MAIN	SEGMENT
	ASSUME	CS:MAIN,DS:MAIN
	MOV	AX,CS
	MOV	DS,AX

L2:	MOV	CX,DATA1[SI]
	MOV	MIN[SI],CX
	ADD	SI,2
	CMP	SI,10
	JE	L3
	JMP	L2

L3:	MOV	SI,0
	JMP	LOOP

LOOP:   MOV	AX,DATA1[DI]
	CMP	SI,8
	JE	L1
	CMP	DI,10
	JE	FIN
	ADD	SI,2
	MOV	BX,DATA1[SI]
	CMP	AX,BX
	JB	LOOP
	CALL	CHANGE
	JMP	LOOP

L1:	ADD	DI,2
	MOV	CX,DI
	MOV	SI,CX
	JMP	LOOP

CHANGE	PROC
	MOV	DATA1[DI],BX
	MOV	DATA1[SI],AX
	MOV	MIN[DI],BX
	MOV	MIN[SI],AX
	RET
CHANGE	ENDP

FIN:	MOV	AH,4CH
	INT	21H

DATA1	DW	35,26,20,3,100
MIN	DW	5 DUP(0)
MAIN	ENDS
	END
